{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["db.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "src": "/register/signin",
        "dest": ".js"
      }
    ]
  }